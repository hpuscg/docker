

1.sudo vim /etc/apt/sources.list.d/docker-main.list
  ----deb [arch=amd64] http://mirrors.aliyun.com/docker-engine/apt/repo ubuntu-xenial main
  
2.curl -fsSL https://apt.dockerproject.org/gpg | sudo apt-key add -

3.apt-get update

4.apt-get purge lxc-docker*

5.apt-cache policy docker-engine

6.apt-get install -y docker-engine=xxxxx
